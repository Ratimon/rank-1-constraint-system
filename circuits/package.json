{
  "name": "circuits",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "snarkjs": "snarkjs",
    "multiply4:compile": "circom multiply4.circom --r1cs --wasm --sym",
    "multiply4:print": "pnpm snarkjs r1cs print ./multiply4.r1cs",
    "multiply4:compute_witness": "node ./multiply4_js/generate_witness.js ./multiply4_js/multiply4.wasm ./multiply4_input.json ./multiply4_js/witness.wtns",
    "multiply4:generate_witness": "pnpm snarkjs wtns export json ./multiply4_js/witness.wtns ./multiply4_js/witness.json",
    "polynomial:compile": "circom ./polynomial/polynomial.circom --r1cs --wasm --sym --output ./polynomial/",
    "polynomial:print": "pnpm snarkjs r1cs print ./polynomial/polynomial.r1cs",
    "polynomial:compute_witness": "node ./polynomial/polynomial_js/generate_witness.js ./polynomial/polynomial_js/polynomial.wasm ./polynomial/polynomial_input.json ./polynomial/polynomial_js/witness.wtns",
    "polynomial:generate_witness": "pnpm snarkjs wtns export json ./polynomial/polynomial_js/witness.wtns ./polynomial/polynomial_js/witness.json",
    "polynomial2:compile": "circom polynomial2.circom --r1cs --wasm --sym",
    "polynomial2:print": "pnpm snarkjs r1cs print ./polynomial2.r1cs",
    "polynomial2:compute_witness": "node ./polynomial2_js/generate_witness.js ./polynomial2_js/polynomial2.wasm ./polynomial2_input.json ./polynomial2_js/witness.wtns",
    "polynomial2:generate_witness": "pnpm snarkjs wtns export json ./polynomial2_js/witness.wtns ./polynomial2_js/witness.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ratimon/rank-1-constraint-system.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ratimon/rank-1-constraint-system/issues"
  },
  "homepage": "https://github.com/Ratimon/rank-1-constraint-system#readme",
  "devDependencies": {
    "circomlib": "^2.0.5",
    "snarkjs": "^0.7.1"
  }
}
