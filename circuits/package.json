{
  "name": "circuits",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "snarkjs": "snarkjs",
    "multiply4:compile": "circom multiply4.circom --r1cs --wasm --sym",
    "multiply4:print": "pnpm snarkjs r1cs print ./multiply4.r1cs",
    "multiply4:compute_witness": "node ./multiply4_js/generate_witness.js ./multiply4_js/multiply4.wasm ./multiply4_js/input.json ./multiply4_js/witness.wtns",
    "multiply4:generate_witness": "pnpm snarkjs wtns export json ./multiply4_js/witness.wtns ./multiply4_js/witness.json",
    "polynomial:compile": "circom polynomial.circom --r1cs --wasm --sym",
    "polynomial:print": "pnpm snarkjs r1cs print ./polynomial.r1cs",
    "polynomial:compute_witness": "node ./polynomial_js/generate_witness.js ./polynomial_js/polynomial.wasm ./polynomial_js/input.json ./polynomial_js/witness.wtns",
    "polynomial:generate_witness": "pnpm snarkjs wtns export json ./polynomial_js/witness.wtns ./polynomial_js/witness.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ratimon/rank-1-constraint-system.git"
